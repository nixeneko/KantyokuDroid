    <Row>
        <Key android:codes="1030" android:keyLabel="z" android:keyEdgeFlags="left"/>
        <Key android:codes="1031" android:keyLabel="x"/>
        <Key android:codes="1032" android:keyLabel="c"/>
        <Key android:codes="1033" android:keyLabel="v"/>
        <Key android:codes="1034" android:keyLabel="b"/>
        <Key android:codes="1035" android:keyLabel="n"/>
        <Key android:codes="1036" android:keyLabel="m"/>
