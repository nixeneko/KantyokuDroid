    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="1010" android:keyLabel="q" android:keyEdgeFlags="left"/>
        <Key android:codes="1011" android:keyLabel="w"/>
