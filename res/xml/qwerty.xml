        <Key android:codes="1018" android:keyLabel="o"/>
        <Key android:codes="1019" android:keyLabel="p" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="1020" android:keyLabel="a" android:keyEdgeFlags="left"/>
        <Key android:codes="1021" android:keyLabel="s"/>
        <Key android:codes="1022" android:keyLabel="d"/>
        <Key android:codes="1023" android:keyLabel="f"/>
        <Key android:codes="1024" android:keyLabel="g"/>
