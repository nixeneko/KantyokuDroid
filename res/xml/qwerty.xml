    <Row android:rowEdgeFlags="bottom">
  	    <!-- <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
                android:keyWidth="10%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/> -->
        <Key android:codes="1041" android:keyLabel="かな" 
                android:isModifier="true"
                android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="10%p" />
        <Key android:codes="-10" android:keyLabel="change" android:keyWidth="10%p" />
        <Key android:codes="21" android:keyLabel="←" android:isRepeatable="true" />
        <Key android:codes="1040" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="22" android:keyLabel="→" android:isRepeatable="true"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="10%p"
                android:isRepeatable="true"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="10%p" android:keyEdgeFlags="right"/>
    </Row>
